<% layout('layouts/boilerplate') -%>

    <main id="product">
        <div class="productbg py-3 container">
            <div class="row">
                <div class="col-md-6 ">
                    <div class="mainframe col-12">
                        <img class="" src="<%=result[0].main_img_path%>" alt="" srcset="">
                        <div class="middle">
                            overlay
                        </div>
                    </div>
                    <div class="row m-2">
                        <div class="col-3 p-0">
                            <div class="subframe active">
                                <img src="<%=result[0].main_img_path %> " alt="" srcset="">
                                <div class="middle">
                                    overlay
                                </div>
                            </div>
                        </div>
                        <% for(let i=0;i<result.length;i++){ %>
                            <div class=" col-3 p-0">
                                <div class="subframe">
                                    <img src="<%=result[i].img_path%>" alt="" srcset="">
                                    <div class="middle">
                                        overlay
                                    </div>
                                </div>
                            </div>
                            <% }%>

                    </div>
                </div>
                <div class="pdesc col-md-6">
                    <h3>
                        <%=result[0].name %>
                    </h3>
                    <h6><b>Style no : </b>
                        <%=result[0].style_no %>
                    </h6>
                    <h6><b>Description : </b>
                        <%=result[0].description %>
                    </h6>
                    <hr>
                    <div id="social-Links" class="text-center">

                        </a>
                        <a class="mr-3 contact-icon skype" title="Skype" target="_BLANK"
                            href="https://join.skype.com/invite/nT3hmhBjNDSB">
                            <i class="fab fa-skype"></i>
                        </a>
                        <a class="mr-3 contact-icon whatsapp" title="Skype" target="_BLANK"
                            href="https://wa.me/+919717456868">
                            <i class="fab fa-whatsapp"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </main>